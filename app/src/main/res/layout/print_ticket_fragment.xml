        <EditText
            android:id="@+id/enterNum"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#FFF"
            android:inputType="textVisiblePassword"
            android:gravity="center"
            android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890 "
            android:hint="LHG999"
            android:textColorHint="#FFC9CACA"
            android:textSize="30dp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="#29568f"
        android:gravity="center"
        android:orientation="horizontal"
        android:padding="10dp"
        android:weightSum="2">

        <Button
            android:id="@+id/bCar"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/caricon"
            android:gravity="center_horizontal"
            android:textColor="#000" />

        <Button
            android:id="@+id/bBike"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/bikeicon"
            android:gravity="center_horizontal"
            android:textColor="#000" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="#29568f"
        android:gravity="center"
        android:orientation="horizontal"
        android:padding="10dp"
        android:weightSum="2">

        <Button
            android:id="@+id/bVan"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/vanicon"
            android:gravity="center_horizontal"
            android:textColor="#000" />

        <Button
            android:id="@+id/bTruck"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:background="@drawable/truckicon"
            android:gravity="center_horizontal"
            android:textColor="#000" />

    </LinearLayout>
